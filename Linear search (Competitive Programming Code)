
#include <stdio.h>
#include <time.h>
#include <stdlib.h>
#define MAX 100
#define MIN 1

int main()
{
    int n, m, key_1, key_2, found_1 = 0, found_2 = 0;
    clock_t start, end;
    double cpu_time;

    printf("\nEnter the size of the first array of random elements: ");
    scanf("%d", &n);

    int arr1[n];

    printf("\nThe array elements are: ");
    for(int i=0; i<n ; i++)
    {
        arr1[i] = rand() % (MAX - MIN + 1) + MIN;
        printf("%d\t", arr1[i]);
    }

    printf("\n\nEnter the element which is to be searched within the first array: ");
    scanf("%d", &key_1);

    printf("\nEnter the size of the second array of random elements: ");
    scanf("%d", &m);

    int arr2[m];

    printf("\nThe array elements are: ");
    for(int i=0; i<m; i++)
    {
        arr2[i] = rand() % (MAX - MIN + 1) + MIN;
        printf("%d\t", arr2[i]);
    }

    printf("\n\nEnter the element which is to be searched within the second array: ");
    scanf("%d", &key_2);

    start = clock();

    for(int i=0; i<n; i++)
    {
        if(key_1 == arr1[i])
        {
            printf("\nThe element %d is at %d position within the first array\n", key_1, i+1);
            found_1 = 1;
            break;
        }
    }

    if(found_1 == 0)
    {
        printf("\nThe element %d is not present within the first array\n", key_1);
    }

    for(int i=0; i<m; i++)
    {
        if(key_2 == arr2[i])
        {
             printf("\nThe element %d is at %d position within the second array\n", key_2, i+1);
             found_2 = 1;
             break;
        }
    }

    if(found_2 == 0)
    {
        printf("\nThe element %d is not present within the second array\n", key_2);
    }

    end = clock();

    cpu_time = (double)(end - start) / CLOCKS_PER_SEC;
    printf("\n%f\n", cpu_time);

    return 0;
}
